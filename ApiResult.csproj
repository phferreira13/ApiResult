<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>ApiResultToolkit</Title>
    <PackageId>Phferreira.ApiResultToolkit</PackageId>
    <Company>Phferreira</Company>
    <Description>ApiResultToolkit: A robust and flexible helper library for handling API responses in .NET. It provides a fluent interface for executing asynchronous operations, validating data, and encapsulating success or failure results. Ideal for building clean and maintainable APIs.</Description>
    <RepositoryUrl>https://github.com/phferreira13/ApiResult</RepositoryUrl>
  </PropertyGroup>

</Project>
